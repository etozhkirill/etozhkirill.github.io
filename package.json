    "jest": "^27.2.1",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.2",
    "sass": "^1.42.1",
